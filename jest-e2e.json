{
  "moduleFileExtensions": ["js", "json", "ts"],
  "testRegex": ".e2e-spec.ts$",
  "rootDir": ".",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverageFrom": ["src/**/*.(t|j)s"],
  "coverageDirectory": "./coverage",
  "testEnvironment": "node",
  "testPathIgnorePatterns": [
    "node_modules/"
  ],
  "modulePaths": [
    "<rootDir>"
  ]
}
